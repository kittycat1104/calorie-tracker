In\">\r\n      <button class=\"advice-btn\" @click=\"getAdvice\" :disabled=\"adviceLoading\">\r\n        {{ adviceLoading ? '取得建議中...' : '獲取飲食建議' }}\r\n      </button>\r\n      <div v-if=\"advice\" class=\"advice-box\">\r\n        <div v-html=\"adviceHtml\"></div>\r\n      </div>\r\n    </div>\r\n    <div v-else>\r\n      <p>請登入以使用完整功能</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport MarkdownIt from 'markdown-it'\r\nimport { mapState, mapMutations } from 'vuex'\r\n\r\nexport default {\r\n  name: 'HomeView',\r\n  computed: {\r\n    ...mapState(['advice', 'adviceHtml', 'token'])\r\n  },\r\n  data() {\r\n    return {\r\n      isLoggedIn: false,\r\n      todayCalories: 0,\r\n      targetCalories: 2000,\r\n      adviceLoading: false\r\n    }\r\n  },\r\n  created() {\r\n    this.isLoggedIn = !!this.token\r\n  },\r\n  methods: {\r\n    ...mapMutations(['setAdvice', 'clearToken', 'clearAdvice']),\r\n    async getAdvice() {\r\n      this.adviceLoading = true\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/api/nutrition/diet-detailed-advice', {\r\n          headers: {\r\n            'Authorization': `Bearer ${this.token}`\r\n          }\r\n        })\r\n        const advice = response.data.advice || response.data.建議 || response.data || '無法取得建議'\r\n        const md = new MarkdownIt({ html: false, breaks: true, linkify: true })\r\n        const adviceHtml = md.render(advice)\r\n        this.setAdvice({ advice, adviceHtml })\r\n      } catch (e) {\r\n        this.setAdvice({ advice: '取得建議失敗，請稍後再試', adviceHtml: '<p>取得建議失敗，請稍後再試</p>' })\r\n      } finally {\r\n        this.adviceLoading = false\r\n      }\r\n    },\r\n    logout() {\r\n      this.clearToken()\r\n      this.clearAdvice()\r\n      // 其他登出流程\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.home {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.dashboard {\r\n  display: flex;\r\n  gap: 20px;\r\n